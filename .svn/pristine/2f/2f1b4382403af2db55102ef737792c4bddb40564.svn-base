<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.genertech.consumer.dao.CZPSMapper">

	<select id="getById" parameterType="java.util.Map"  resultType="String">
		select id from CZPS t where  id=#{id,jdbcType=VARCHAR} 
	</select>
	<select id="getCpzdById" parameterType="java.util.Map"  resultType="String">
		select id from cpzd t where  id=#{id,jdbcType=VARCHAR} 
	</select>
	<select id="getPjzdById" parameterType="java.util.Map"  resultType="String">
		select id from pjzd t where  id=#{id,jdbcType=VARCHAR} 
	</select>
	<select id="getTzbmById" parameterType="java.util.Map"  resultType="String">
		select id from tzbm t where  id=#{id,jdbcType=VARCHAR} 
	</select>
	<select id="getIds"   resultType="String">
		select id from CZPS_test t 
	</select>
	<select id="getCpzdIds"   resultType="String">
		select id from CPZD_test t 
	</select>
	<select id="getPjzdIds"   resultType="String">
		select id from PJZD_test t 
	</select>
	<select id="getTzbmIds"   resultType="String">
		select id from TZBM_test t 
	</select>
	<update id="updateCzps" parameterType="java.util.List">
        <foreach collection="list" item="bean" index="index" open="" close="" separator=";">
            UPDATE CZPS
            	set AA=#{bean.AA,jdbcType=CHAR},AB=#{bean.AB,jdbcType=VARCHAR},AC=#{bean.AC,jdbcType=CHAR},AD=#{bean.AD,jdbcType=DATE},AE=#{bean.AE,jdbcType=DATE},
            	AF=#{bean.AF,jdbcType=CHAR},AG=#{bean.AG,jdbcType=VARCHAR},AH=#{bean.AH,jdbcType=VARCHAR},AI=#{bean.AI,jdbcType=CHAR},AJ=#{bean.AJ,jdbcType=VARCHAR},
            	ZV=#{bean.ZV,jdbcType=DATE},ZW=#{bean.ZW,jdbcType=DATE},ZX=#{bean.ZX,jdbcType=VARCHAR},ZY=#{bean.ZY,jdbcType=VARCHAR},ZZ=#{bean.ZZ,jdbcType=CHAR},
            	BA=#{bean.BA,jdbcType=CHAR},BF=#{bean.BF,jdbcType=CHAR},AK=#{bean.AK,jdbcType=CHAR},AL=#{bean.AL,jdbcType=INTEGER},SA=#{bean.SA,jdbcType=VARCHAR},
            	SB=#{bean.SB,jdbcType=VARCHAR},CA=#{bean.CA,jdbcType=VARCHAR},CB=#{bean.CB,jdbcType=VARCHAR},FT=#{bean.FT,jdbcType=VARCHAR},BB=#{bean.BB,jdbcType=INTEGER},
            	BC=#{bean.BC,jdbcType=INTEGER} 
                where id = #{bean.id,jdbcType=VARCHAR}
        </foreach>
    </update>
	<update id="updateCpzd" parameterType="java.util.List">
        <foreach collection="list" item="bean" index="index" open="" close="" separator=";">
            UPDATE CPZD
				set AA=#{bean.AA,jdbcType=CHAR},AB=#{bean.AB,jdbcType=VARCHAR},AC=#{bean.AC,jdbcType=CHAR},AD=#{bean.AD,jdbcType=VARCHAR},AE=#{bean.AE,jdbcType=VARCHAR},
            	AF=#{bean.AF,jdbcType=VARCHAR},AG=#{bean.AG,jdbcType=INTEGER},AH=#{bean.AH,jdbcType=VARCHAR},AI=#{bean.AI,jdbcType=VARCHAR},ZV=#{bean.ZV,jdbcType=DATE},
            	ZW=#{bean.ZW,jdbcType=DATE},ZX=#{bean.ZX,jdbcType=CHAR},ZY=#{bean.ZY,jdbcType=CHAR},ZZ=#{bean.ZZ,jdbcType=CHAR},AJ=#{bean.AJ,jdbcType=INTEGER},AK=#{bean.AK,jdbcType=INTEGER},
            	AL=#{bean.AL,jdbcType=INTEGER},AM=#{bean.AM,jdbcType=INTEGER},AN=#{bean.AN,jdbcType=INTEGER},AO=#{bean.AO,jdbcType=INTEGER},AP=#{bean.AP,jdbcType=INTEGER},AQ=#{bean.AQ,jdbcType=INTEGER},
            	AR=#{bean.AR,jdbcType=INTEGER},AU=#{bean.AU,jdbcType=VARCHAR},BA=#{bean.BA,jdbcType=VARCHAR}
                were id = #{bean.id,jdbcType=VARCHAR}
        </foreach>
    </update>
    <update id="updatePjzd" parameterType="java.util.List">
        <foreach collection="list" item="bean" index="index" open="" close="" separator=";">
            UPDATE PJZD
            	set AA=#{bean.AA,jdbcType=VARCHAR},AB=#{bean.AB,jdbcType=VARCHAR},AC=#{bean.AC,jdbcType=CHAR},AD=#{bean.AD,jdbcType=VARCHAR},AE=#{bean.AE,jdbcType=VARCHAR},
            	ZV=#{bean.ZV,jdbcType=DATE},ZW=#{bean.ZW,jdbcType=DATE},ZX=#{bean.ZX,jdbcType=VARCHAR},ZY=#{bean.ZY,jdbcType=VARCHAR},ZZ=#{bean.ZZ,jdbcType=CHAR},
            	AF=#{bean.AF,jdbcType=VARCHAR},AG=#{bean.AG,jdbcType=VARCHAR},BB=#{bean.BB,jdbcType=CHAR},PID=#{bean.PID,jdbcType=VARCHAR},ISPARENT=#{bean.ISPARENT,jdbcType=CHAR}
                where id = #{bean.id}
        </foreach>
    </update>
    <update id="updateTzbm" parameterType="java.util.List">
        <foreach collection="list" item="bean" index="index" open="" close="" separator=";">
            UPDATE TZBM
            	set AA=#{bean.AA,jdbcType=VARCHAR},ZZ=#{bean.ZZ,jdbcType=CHAR}<!-- ,AB=#{bean.AB},AC=#{bean.AC}ZV=#{bean.ZV},
            	ZW=#{bean.ZW},ZX=#{bean.ZX},ZY=#{bean.ZY}, -->
             where   id = #{bean.id}
        </foreach>
    </update>
</mapper>